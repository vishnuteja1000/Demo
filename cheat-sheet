touch .gitignore
git config --global user.name vishnu.teja1000
git config --global user.email vishnu.teja1000@gmail.com
git init
git add .
git commit -m "Initial Commit"
git status
ssh-keygen -t ed25519 -C "vishnu.teja1000@gmail.com"
tail #add key
eval `ssh-agent -s`
git remote add origin [ssh url.git]

ssh-add ~/.ssh/id_ed25519
cat ~/.ssh/id_ed25519.pub
